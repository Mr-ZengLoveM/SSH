<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="struts-orders" extends="struts2">
		<action name="ordersAction_*" class="ordersAction" method="{1}">
			<result type="redirect" name="getAllSuc">testOrders/selectAll.jsp</result>
			<result type="redirect" name="addSuc">testOrders/selectAll.jsp</result>
			<result type="redirect" name="delectSuc">testOrders/selectAll.jsp</result>
			<result name="input">testOrders/error.jsp</result>
		</action>
	</package>
</struts>    
